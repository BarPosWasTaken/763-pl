    <style>
        body{
            background: #34495E;
        }
        .centered{
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%,-50%);
            width: fit-content;
            height: fit-content;
            max-width: 100vw;
            max-height: 100vh;
            overflow: visible;
        }

        .input {
            box-shadow:none;
            margin-top: 10px;
            width: 100%;
            font-family: inherit;
            padding-bottom: 1rem;
            background: none;
            border: none;
            color: #ffffff;
            border-bottom: 2px solid #ffffff;
            text-align: center;
            font-size: 1.25rem;
            transition: border-bottom-color 0.3s ease-in-out;
            caret-color: #3498DB;
        }

        .input:focus {
            outline: none;
            border-bottom-color: #3498DB;
        }

        .input::placeholder {
            color: #ffffff;
            opacity: 0.7;
        }

        .input,
        .create {
            margin: 1rem 0;
            max-width: 100%;
        }
        .create {
            width: 100%;
            cursor: pointer;
            font-family: inherit;
            font-size: 1.15rem;
            color: #fff;
            border: none;
            background-color: #3498DB;
            padding: 0.75em 1.25rem;
            box-shadow: 3px 3px 0 0 #ffffff;
            transition: box-shadow 0.2s ease-in-out;
        }

        .create:hover {
            box-shadow: 0 0 0 0 #ffffff;
        }

        .created {
            color: #ffffff;
            text-align: center;
        }

        .created a {
            color: inherit;
        }


        .error {
            background: #A63446;
            padding: 0.5rem 1rem;
            color: #ffffff;
            margin-bottom: 1rem;
            text-align: center;
        }
        .copyright{
            position: fixed;
            bottom: 0;
            left: 5px;
            color: white;
            z-index: 9999;
        }
        a{
            color: white;
            text-decoration: none;
        }
        a:hover{
            color: white;
            text-decoration: underline;
        }
